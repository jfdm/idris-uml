device server presentation {
    env appserver presentation {
        artifact exe policyAdminUI.war with spec called spec.xml {
            a : "asas"
            b : "asas"
        }
    }
    properties {
        a : "sdsd"
        b : "asas"
    }

}

device server policy {
    env appserver policy {
        artifact exe policyServer.jar
        properties {
            a : "sdsd"
            b : "asas"
        }
    }
}

device server product {
    env appserver product {
        artifact exe productServer.jar
    }

    env engine rules {
        artifact doc rules
    }
}

device workstation user {
    env os workstation {
        artifact exe productServerUI.exe
    }
}

device server documents {
    env os server {
        artifact exe docMngmntSystem
    }
}

device server dbServer {
    env engine dbms {
        artifact doc schema_product
        artifact doc schema_policy
    }
}

device server dirServer {
    env app ldapServer {
        artifact GrpUsrheirarchy
    }
}

device server uWriteRating {
    env engine rules {
        artifact doc rating_rules
        artifact doc under_writing_rules
    }
    env appserver apps {
        artifact app under_writing_engine.jar
        artifact app rating_engine.jar
    }
}

presentation commsWith policy via http
policy commsWith uWriteRating via http
policy commsWith dirServer via http
policy commsWith dbServer via http
policy commsWith docServer via http
dbServer commsWith product via http
product commsWith docServer via http
user commsWith product via http
